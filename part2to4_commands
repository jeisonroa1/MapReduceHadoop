mkdir proj_part1/
mkdir proj_part1/part2/input
mkdir proj_part1/part2/output
mkdir proj_part1/part3/input
mkdir proj_part1/part3/output
mkdir proj_part1/part4/input
mkdir proj_part1/part4/output
cp orders proj_part1/part2/input
cp orders proj_part1/part3/input
cp orders proj_part1/part4/input

hadoop fs -mkdir /user/cloudera
hadoop fs -mkdir /user/cloudera/rf
hadoop fs -mkdir /user/cloudera/rf/input
hadoop fs -mkdir /user/cloudera/rf/pair
hadoop fs -mkdir /user/cloudera/rf/pair/basic
hadoop fs -mkdir /user/cloudera/rf/stripe
hadoop fs -mkdir /user/cloudera/rf/stripe/basic
hadoop fs -mkdir /user/cloudera/rf/hybrid

hadoop fs -put orders /user/cloudera/rf/input

hadoop jar relfreq.jar org.myorg.RelFreq /user/cloudera/rf/input /user/cloudera/rf/pair/basic/output
hdfs dfs -get /user/cloudera/rf/pair/basic/output/* proj_part1/part2/output

hadoop jar relfreqstripe.jar org.myorg.RelFreqStripe /user/cloudera/rf/input /user/cloudera/rf/stripe/basic/output
hdfs dfs -get /user/cloudera/rf/stripe/basic/output/* proj_part1/part3/output

hadoop jar relfreqhybrid.jar org.myorg.RelFreqHybrid /user/cloudera/rf/input /user/cloudera/rf/hybrid/output
hdfs dfs -get /user/cloudera/rf/hybrid/output/* proj_part1/part4/output

